@model IEnumerable<VTP2015.Modules.Lecturer.ViewModels.PartimListViewModel>

<div class="panel panel-success clearfix partimcontainer">
    <div class="panel-heading">
        <h3 class="panel-title">Partims</h3>
    </div>
    <div class="panel-body" id="partimPointerDiv">
        <ul class="list-group">
            @foreach (var partim in Model)
            {
                <li class="list-group-item partimpointer" data-moduleid="@partim.PartimId"><span class="name">@partim.PartimName</span></li>
            }
        </ul>
    </div>
</div>
