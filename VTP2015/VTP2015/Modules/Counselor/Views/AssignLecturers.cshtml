@model VTP2015.Modules.Counselor.ViewModels.PartimInformationViewModel[]
    
<div class="container body-content">
    <div class="container-fix">
        <div class="panel panel-info clearfix partimcontainer">
            <div class="panel-heading">
                <h3 class="panel-title">Partims</h3>
            </div>
            <div class="panel-body">
                <ul class="list-group">
                    @foreach (var partim in Model)
                    {
                        <li id="@partim.SuperCode" class="list-group-item">
                            <b>@partim.ModuleName - @partim.PartimName</b><input class="email" type="email"/>
                        </li>
                    }
                </ul>
            </div>
        </div>
    </div>
</div>